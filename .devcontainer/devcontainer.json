{
  "image": "mcr.microsoft.com/devcontainers/universal:5",
  "hostRequirements": {
    "cpus": 4
  },
  "waitFor": "onCreateCommand",
  "postCreateCommand": "pnpm install",
  "postAttachCommand": {
    "server": "pnpm run dev"
  },
  "portsAttributes": {
    "3000": {
      "label": "Application",
      "onAutoForward": "openPreview"
    }
  },
  "features": {
    "ghcr.io/wandb/catnip/feature:1": {}
  },
  "forwardPorts": [3000, 6369]
}
