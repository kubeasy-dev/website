    "@types/react-dom": "19.1.6",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "feed": "5.1.0",
    "framer-motion": "12.18.1",
    "fumadocs-core": "15.5.3",
    "fumadocs-mdx": "11.6.9",
    "fumadocs-ui": "15.5.3",
    "jotai": "2.12.5",
    "lucide-react": "0.518.0",
    "next": "15.3.4",
    "next-themes": "0.4.6",
    "octokit": "5.0.3",
    "posthog-js": "1.255.0",
