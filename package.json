{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "gen:types": "npx supabase gen types typescript --linked --schema=public > lib/database.types.ts",
    "knip": "knip",
    "prepare": "husky install",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "next lint"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@opennextjs/cloudflare": "^1.0.2",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.5",
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-dropdown-menu": "^2.1.5",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.5",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@supabase-cache-helpers/postgrest-react-query": "^1.13.2",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-query": "^5.74.3",
    "@tanstack/react-query-devtools": "^5.74.3",
    "@tanstack/react-table": "^8.21.3",
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.3",
    "autoprefixer": "10.4.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.4.0",
    "jotai": "^2.12.3",
    "lucide-react": "^0.446.0",
    "next": "15.4.0-canary.30",
    "next-themes": "^0.4.4",
    "postcss": "8.4.30",
    "posthog-js": "^1.236.4",
    "posthog-node": "5.0.0-alpha.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.54.2",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "20.6.2",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "eslint": "9.24.0",
    "eslint-config-next": "15.4.0-canary.30",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^8.0.0",
    "knip": "^5.50.4",
    "prettier": "^3.5.3",
    "supabase": "^2.9.6",
    "typescript": "5.2.2",
    "typescript-eslint": "^8.30.1",
    "wrangler": "^4.14.4"
  },
  "overrides": {
    "eslint": "$eslint"
  },
  "peerDependencies": {
    "axios": ""
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.1.3",
      "@types/react-dom": "19.1.3"
    },
    "onlyBuiltDependencies": [
      "core-js",
      "core-js-pure",
      "sharp",
      "supabase"
    ]
  }
}
