  "pnpm": {
    "overrides": {
      "@types/react": "19.1.8",
      "@types/react-dom": "19.1.6"
    },
    "onlyBuiltDependencies": [
      "core-js",
      "core-js-pure",
      "esbuild",
      "sharp",
      "supabase",
      "workerd",
      "unrs-resolver"
    ]
  },
  "lint-staged": {
    "*/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "eslint"
    ],
    "*/**/*.{json,css,md}": [
      "prettier --write"
    ]
  }
